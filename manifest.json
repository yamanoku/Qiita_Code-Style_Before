{
  "name": "Qiita Code Style Before",
  "version": "0.1",
  "description" : "Qiitaのcode部分のスタイルを旧デザインにするやつ",
  "manifest_version": 2,
  "permissions":["tabs","activeTab","https://qiita.com/*"],
  "icons": {
    "16": "assets/icon-16.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "browser_action": {
    "default_icon": "assets/icon-16.png"
  },
  "content_scripts": [
    {
      "matches": ["https://qiita.com/*"],
      "js": ["scripts.js"]
    }
  ],
  "web_accessible_resources": [
    "scripts.js",
    "style.css"
  ]
}
